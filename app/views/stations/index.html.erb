<h1>Listing stations</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Space</th>
    <th>Idle</th>
    <th>Opentime</th>
    <th>Closetime</th>
    <th>Cityid</th>
    <th>Localid</th>
    <th>Manual</th>
    <th>Functioning</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stations.each do |station| %>
  <tr>
    <td><%= station.name %></td>
    <td><%= station.address %></td>
    <td><%= station.latitude %></td>
    <td><%= station.longitude %></td>
    <td><%= station.space %></td>
    <td><%= station.idle %></td>
    <td><%= station.openTime %></td>
    <td><%= station.closeTime %></td>
    <td><%= station.cityId %></td>
    <td><%= station.localId %></td>
    <td><%= station.manual %></td>
    <td><%= station.functioning %></td>
    <td><%= link_to 'Show', station %></td>
    <td><%= link_to 'Edit', edit_station_path(station) %></td>
    <td><%= link_to 'Destroy', station, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Station', new_station_path %>
